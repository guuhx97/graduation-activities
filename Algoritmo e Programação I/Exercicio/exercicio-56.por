programa
{
	funcao inicio()
	{
		
		para(inteiro c= 0;c<5;c++){
			para(inteiro i= 0;i<5;i++){
				se(c > i){
					escreva(" 1 ")
				}senao{
					escreva(" 0 ")
				}
				
			}
			escreva("\n")
		}
		
	}
}
